{
  "name": "toboggan-ws",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "concurrently \"npm run start:app\" \"npm run start:api\"",
    "start:app": "nx serve",
    "start:api": "nx run toboggan-api:serve:development",
    "build": "nx build",
    "test": "nx test",
    "e2e:test:debug": "ng e2e toboggan-app-e2e --watch",
    "prepare": "husky install",
    "app:lint": "nx run toboggan-app:lint --fix",
    "api:lint": "nx run toboggan-api:lint --fix",
    "tsc": "yarn app:tsc && yarn api:tsc",
    "app:tsc": "tsc -p apps/toboggan-app/tsconfig.json --noEmit --incremental",
    "api:tsc": "tsc -p apps/toboggan-api/tsconfig.json --noEmit --incremental"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.1.1",
    "@angular/common": "14.1.1",
    "@angular/compiler": "14.1.1",
    "@angular/core": "14.1.1",
    "@angular/forms": "14.1.1",
    "@angular/platform-browser": "14.1.1",
    "@angular/platform-browser-dynamic": "14.1.1",
    "@angular/router": "14.1.1",
    "@nestjs/common": "9.0.8",
    "@nestjs/core": "9.0.8",
    "@nestjs/mapped-types": "^1.1.0",
    "@nestjs/platform-express": "9.0.8",
    "@nrwl/angular": "14.5.4",
    "@snhuproduct/toboggan-ui-components-library": "^1.4.20",
    "@types/uuid": "^8.3.4",
    "array-paginate": "^1.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "concurrently": "^7.3.0",
    "husky": "^8.0.1",
    "jest-canvas-mock": "^2.4.0",
    "jest-mock-extended": "^2.0.7",
    "ngx-bootstrap": "^9.0.0",
    "pretty-quick": "^3.1.3",
    "ramda": "^0.28.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.1",
    "@angular-eslint/eslint-plugin": "~14.0.0",
    "@angular-eslint/eslint-plugin-template": "~14.0.0",
    "@angular-eslint/template-parser": "~14.0.0",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "14.1.1",
    "@angular/language-service": "14.1.1",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.8",
    "@nrwl/cli": "14.5.4",
    "@nrwl/cypress": "14.5.4",
    "@nrwl/eslint-plugin-nx": "14.5.4",
    "@nrwl/jest": "14.5.4",
    "@nrwl/linter": "14.5.4",
    "@nrwl/nest": "14.5.4",
    "@nrwl/node": "14.5.4",
    "@nrwl/workspace": "14.5.4",
    "@types/jest": "27.4.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "16.11.7",
    "@types/ramda": "^0.28.15",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "cypress": "^9.1.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.5.1",
    "jest-preset-angular": "~11.1.2",
    "lodash": "^4.17.21",
    "nx": "14.5.4",
    "prettier": "^2.6.2",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2"
  }
}
